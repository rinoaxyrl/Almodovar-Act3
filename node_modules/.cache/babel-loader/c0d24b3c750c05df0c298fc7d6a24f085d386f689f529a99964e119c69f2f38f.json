{"ast":null,"code":"export default {\n  name: 'ShoppingCart',\n  data() {\n    return {\n      products: [{\n        id: 1,\n        name: 'Milk',\n        price: 135\n      }, {\n        id: 2,\n        name: 'Egg',\n        price: 72\n      }, {\n        id: 3,\n        name: 'Cheese',\n        price: 63\n      }, {\n        id: 4,\n        name: 'Yogurt',\n        price: 25\n      }, {\n        id: 5,\n        name: 'Pasta',\n        price: 75\n      }, {\n        id: 6,\n        name: 'Rice',\n        price: 52\n      }, {\n        id: 7,\n        name: 'Bread',\n        price: 65\n      }, {\n        id: 8,\n        name: 'Oats',\n        price: 89\n      }, {\n        id: 9,\n        name: 'Pork',\n        price: 175\n      }, {\n        id: 10,\n        name: 'Beef',\n        price: 203\n      }],\n      cart: [],\n      total: 0\n    };\n  },\n  methods: {\n    addToCart(product) {\n      let found = this.cart.find(item => item.product.id === product.id);\n      if (found) {\n        found.quantity++;\n      } else {\n        this.$set(this.cart, this.cart.length, {\n          product: product,\n          quantity: 1,\n          total: product.price\n        });\n      }\n      this.calculateTotal();\n    },\n    removeFromCart(index) {\n      this.cart.splice(index, 1);\n      this.calculateTotal();\n    },\n    updateQuantity(item) {\n      item.total = item.product.price * item.quantity;\n      this.calculateTotal();\n    },\n    calculateTotal() {\n      let total = 0;\n      this.cart.forEach(item => {\n        total += item.total;\n      });\n      this.total = total;\n    }\n  }\n};","map":{"version":3,"names":["name","data","products","id","price","cart","total","methods","addToCart","product","found","find","item","quantity","$set","length","calculateTotal","removeFromCart","index","splice","updateQuantity","forEach"],"sources":["C:\\Users\\Tatay\\vueproject\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <h1>Shopping Cart</h1>\n    <hr>\n\n    <div>\n      <h2>Products</h2>\n      <ul>\n        <li v-for=\"product in products\" :key=\"product.id\">\n          {{ product.name }} - ${{ product.price }}\n          <button class=\"add-to-cart\" @click=\"addToCart(product)\">Add to Cart</button>\n        </li>\n      </ul>\n    </div>\n\n    <div>\n      <h2>Cart</h2>\n      <table>\n        <thead>\n          <tr>\n            <th>Product</th>\n            <th>Price</th>\n            <th>Quantity</th>\n            <th>Action</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(item, index) in cart\" :key=\"index\">\n            <td>{{ item.product.name }}</td>\n            <td>${{ item.product.price }}</td>\n            <td>\n              <input type=\"number\" v-model=\"item.quantity\" @change=\"updateQuantity(item)\">\n            </td>\n            <td><button class=\"remove-from-cart\" @click=\"removeFromCart(index)\">Remove</button></td>\n          </tr>\n        </tbody>\n      </table>\n      <p>Total: â‚±{{ total }}</p>\n    </div>\n\n  </div>\n</template>\n\n<script>\nexport default {\nname: 'ShoppingCart',\ndata() {\n  return {\n    products: [\n      { id: 1, name: 'Milk', price: 135 },\n      { id: 2, name: 'Egg', price:72 },\n      { id: 3, name: 'Cheese', price: 63 },\n      { id: 4, name: 'Yogurt', price: 25 },\n      { id: 5, name: 'Pasta', price: 75 },\n      { id: 6, name: 'Rice', price: 52 },\n      { id: 7, name: 'Bread', price: 65 },\n      { id: 8, name: 'Oats', price: 89 },\n      { id: 9, name: 'Pork', price: 175 },\n      { id: 10, name: 'Beef', price: 203 }\n    ],\n    cart: [],\n    total: 0\n  };\n},\nmethods: {\n  addToCart(product) {\n    let found = this.cart.find(item => item.product.id === product.id);\n    if (found) {\n      found.quantity++;\n    } else {\n      this.$set(this.cart, this.cart.length, { product: product, quantity: 1, total: product.price });\n    }\n    this.calculateTotal();\n  },\n  removeFromCart(index) {\n    this.cart.splice(index, 1);\n    this.calculateTotal();\n  },\n  updateQuantity(item) {\n    item.total = item.product.price * item.quantity;\n    this.calculateTotal();\n  },\n  calculateTotal() {\n    let total = 0;\n    this.cart.forEach(item => {\n      total += item.total;\n    });\n    this.total = total;\n  }\n}\n};\n</script>\n\n\n<style scoped>\nbody {\n  font-family: Arial, sans-serif;\n  margin: 0;\n  padding: 0;\n  background-color: #f4f4f4;\n}\n\n#app {\n  max-width: 800px;\n  margin: 0 auto;\n  padding: 20px;\n  background-color: #fff;\n  border-radius: 8px;\n  box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);\n}\n\nh1, h2 {\n  text-align: center;\n}\n\nul {\n  list-style: none;\n  padding: 0;\n}\n\nli {\n  padding: 10px;\n  margin-bottom: 5px;\n  background-color: #f9f9f9;\n  border-radius: 4px;\n}\n\nbutton {\n  padding: 5px 10px;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n}\n\nbutton.add-to-cart {\n  background-color: #cbc3e3;\n  color: #000;\n}\n\nbutton.remove-from-cart {\n  background-color: #dc143c;\n  color: #fff;\n}\n\ntable {\n  width: 100%;\n  border-collapse: collapse;\n}\n\nth {\n  padding: 8px;\n  text-align: left;\n  background-color: #cbc3e3;\n  color: #000;\n}\n\ntd {\n  padding: 8px;\n  text-align: left;\n}\n\ntbody tr:nth-child(even) {\n  background-color: #f2f2f2;\n}\n\ninput[type=\"number\"] {\n  width: 50px;\n  padding: 5px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n}\n\np {\n  text-align: right;\n  margin-top: 10px;\n}\n</style>\n"],"mappings":"AA4CA,eAAe;EACfA,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEH,IAAI,EAAE,MAAM;QAAEI,KAAK,EAAE;MAAI,CAAC,EACnC;QAAED,EAAE,EAAE,CAAC;QAAEH,IAAI,EAAE,KAAK;QAAEI,KAAK,EAAC;MAAG,CAAC,EAChC;QAAED,EAAE,EAAE,CAAC;QAAEH,IAAI,EAAE,QAAQ;QAAEI,KAAK,EAAE;MAAG,CAAC,EACpC;QAAED,EAAE,EAAE,CAAC;QAAEH,IAAI,EAAE,QAAQ;QAAEI,KAAK,EAAE;MAAG,CAAC,EACpC;QAAED,EAAE,EAAE,CAAC;QAAEH,IAAI,EAAE,OAAO;QAAEI,KAAK,EAAE;MAAG,CAAC,EACnC;QAAED,EAAE,EAAE,